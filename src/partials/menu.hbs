{{#if pages}}
	<nav class="block-navigation__menu block-navigation__menu--breakout">
		<ul class="navigation-menu">
			{{#each pages}}
				<li>
					<a href="{{../baseUrl}}{{url}}" {{getLinkActiveState this.url ../currentPageUrl}}>
						{{title}}
					</a>
				</li>
			{{/each}}
		</ul>
	</nav>
{{/if}}
